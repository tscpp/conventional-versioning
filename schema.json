{
  "$schema": "https://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "onlyWorkspaceProtocol": {
      "type": "boolean"
    },
    "overrideComplexRange": {
      "type": "boolean"
    },
    "updateStableToPreRelease": {
      "type": "boolean"
    },
    "ignoreOutdatedPreRelease": {
      "type": "boolean"
    },
    "ignoreInvalidCommit": {
      "type": "boolean"
    },
    "initialPreRelease": {
      "type": "number"
    },
    "preservePreRelease": {
      "type": "boolean"
    },
    "includePrivate": {
      "type": "boolean"
    },
    "allowFirstMajor": {
      "type": "boolean"
    },
    "preventMajorBump": {
      "type": "boolean"
    },
    "releaseTypes": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "enum": ["patch", "minor", "major"]
      }
    },
    "include": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "exclude": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "linked": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "fixed": {
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "base": {
      "type": "string"
    },
    "preReleases": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "promotions": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    }
  },
  "required": ["base"]
}
